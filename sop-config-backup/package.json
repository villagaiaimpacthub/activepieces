{
  "name": "activepieces-sop-tool",
  "version": "1.0.0",
  "description": "A client-facing visual SOP design tool built on Activepieces framework",
  "scripts": {
    "build": "nx build",
    "build:frontend": "nx build ui-frontend",
    "build:backend": "nx build backend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "nx serve ui-frontend",
    "dev:backend": "nx serve backend",
    "start": "nx serve",
    "start:prod": "node dist/apps/backend/main.js",
    "test": "nx test",
    "test:frontend": "nx test ui-frontend",
    "test:backend": "nx test backend",
    "test:e2e": "nx e2e",
    "lint": "nx lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "create-piece": "nx generate @activepieces/cli:create-piece",
    "migrations:generate": "typeorm migration:generate -d src/database/database-config.ts",
    "migrations:run": "typeorm migration:run -d src/database/database-config.ts",
    "db:reset": "npm run db:drop && npm run db:create && npm run migrations:run",
    "db:create": "createdb activepieces_sop",
    "db:drop": "dropdb --if-exists activepieces_sop"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@nestjs/common": "^10.2.8",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.8",
    "@nestjs/platform-socket.io": "^10.2.8",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.9",
    "@angular-devkit/core": "^16.2.9",
    "@angular-devkit/schematics": "^16.2.9",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
