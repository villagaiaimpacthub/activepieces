    "@angular/compiler-cli": "^16.2.12",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@nx/angular": "^17.1.3",
    "@nx/eslint": "^17.1.3",
    "@nx/eslint-plugin": "^17.1.3",
    "@nx/jest": "^17.1.3",
    "@nx/js": "^17.1.3",
    "@nx/nest": "^17.1.3",
    "@nx/node": "^17.1.3",
    "@nx/workspace": "^17.1.3",
    "@types/bcryptjs": "^2.4.5",
    "@types/express": "^4.17.20",
